{
    "name": "zorachka/command-bus",
    "description": "Zorachka Command Bus",
    "keywords": [
        "zorachka"
    ],
    "homepage": "https://github.com/zorachka/zorachka",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "zorachka/container": "^3.1",
        "league/tactician": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Zorachka\\CommandBus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zorachka\\CommandBus\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
