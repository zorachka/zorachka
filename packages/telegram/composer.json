{
    "name": "zorachka/telegram",
    "description": "Zorachka Telegram",
    "keywords": [
        "zorachka",
        "telegram"
    ],
    "homepage": "https://github.com/zorachka/telegram",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "telegram-bot/api": "^2.4",
        "zorachka/console": "^1.1",
        "zorachka/container": "^3.1",
        "zorachka/http": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Zorachka\\Telegram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zorachka\\Telegram\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
